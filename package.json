    "lint": "eslint ./lib ./test",
    "docs": "documentation build ./lib -f md -o docs/documentation.md"
  },
  "homepage": "https://chirashijs.org",
  "author": {
    "name": "Alex Toudic",
    "mail": "alex@chirashijs.org",
    "url": "https://alextoudic.xyz"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chirashijs/chirashi.git"
  },
  "bugs": {
    "url": "https://github.com/chirashijs/chirashi/issues"
  },
  "devDependencies": {
    "babelrc-rollup": "^3.0.0",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "karma": "^1.4.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1"
  },
  "dependencies": {},
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
