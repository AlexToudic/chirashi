{
  "name": "chirashi",
  "version": "6.0.2",
  "description": "Minimalist DOM and events manager focused on performances",
  "keywords": [
    "DOM",
    "events"
  ],
  "main": "./dist/chirashi.common.js",
  "module": "./dist/chirashi.mjs",
  "jsnext:main": "./dist/chirashi.mjs",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "node build/build.js",
    "dev:test": "NODE_ENV=test karma start build/karma.dev.conf.js",
    "test:unit": "NODE_ENV=test karma start build/karma.unit.conf.js",
    "test:cover": "NODE_ENV=test karma start build/karma.cover.conf.js",
    "test": "npm run lint && npm run test:cover",
    "analyze": "bash build/analyze.sh",
    "lint": "eslint ./lib ./test",
    "docs": "npm run docs:md",
    "docs:md": "documentation build ./lib -f md -o docs/documentation.md",
    "docs:html": "documentation build ./lib -f html -o docs/www",
    "release": "bash build/release.sh"
  },
  "homepage": "https://chirashijs.org",
  "author": {
    "name": "Alex Toudic",
    "mail": "alex@chirashijs.org",
    "url": "https://alextoudic.xyz"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chirashijs/chirashi.git"
  },
  "bugs": {
    "url": "https://github.com/chirashijs/chirashi/issues"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-es6-shim": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^2.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "uglify-js": "^2.7.5",
    "webpack": "^2.2.0"
  },
  "dependencies": {},
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
